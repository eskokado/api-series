{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/Api.Application/application.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "publish",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/Api.Application/application.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "watch",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run",
                "${workspaceFolder}/Api.Application/application.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
          "label": "Api.Infra.Data.Tests",
          "command": "dotnet",
          "type": "process",
          "args": [
            "test",
            "${workspaceFolder}/Api.Infra.Data.Test/Api.Infra.Data.Test.csproj",
          ]
        },
        {
          "label": "Api.Service.Tests",
          "command": "dotnet",
          "type": "process",
          "args": [
            "test",
            "${workspaceFolder}/Api.Service.Test/Api.Service.Test.csproj",
          ]
        },
        {
          "label": "Api.Application.Tests",
          "command": "dotnet",
          "type": "process",
          "args": [
            "test",
            "${workspaceFolder}/Api.Application.Test/Api.Application.Test.csproj",
          ]
        }
    ]
}